@Library('library')_
pipeline
{
   agent any
   stages
   {
      stage('ContDownload_master')
      {
         steps
	 {
	     script
	     {
	        cicd.newGit("maven")
	     }
	 }
      }
      stage('ContBuild_master')
      {
         steps
	 {
	     script
	     {
	        cicd.newBuild()
	     }
	 }
      }
   }
}
